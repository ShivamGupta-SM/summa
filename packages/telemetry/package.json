{
	"name": "@summa/telemetry",
	"version": "0.1.0",
	"type": "module",
	"license": "MIT",
	"sideEffects": false,
	"engines": {
		"node": ">=22.0.0"
	},
	"publishConfig": {
		"access": "public"
	},
	"exports": {
		".": {
			"dev-source": "./src/index.ts",
			"types": "./dist/index.d.mts",
			"default": "./dist/index.mjs"
		}
	},
	"files": [
		"dist"
	],
	"scripts": {
		"build": "tsdown",
		"dev": "tsdown --watch",
		"test": "vitest run",
		"typecheck": "tsc --project tsconfig.json",
		"lint:package": "publint --strict",
		"lint:types": "attw --profile esm-only --pack .",
		"prepublishOnly": "tsdown"
	},
	"devDependencies": {
		"@arethetypeswrong/cli": "catalog:",
		"@types/node": "catalog:",
		"publint": "catalog:",
		"tsdown": "catalog:",
		"typescript": "catalog:",
		"vitest": "catalog:"
	}
}
